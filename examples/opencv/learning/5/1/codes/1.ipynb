    "import cv2\n",
    "import jajucha2\n",
    "\n",
    "# 이미지를 불러옵니다.\n",
    "image = cv2.imread(\"example.png\")\n",
    "\n",
    "# 평균 블러링을 적용합니다. (커널 크기 5x5)\n",
    "average_blurred = cv2.blur(image, (5, 5))\n",
    "\n",
    "# 결과를 출력합니다.\n",
    "jajucha2.camera.show_image(average_blurred, \"center\")"
   ]
  }
